{
  "extends": ["../../.eslintrc.js", "plugin:node/recommended"],
    "root": false,
    "parserOptions": {
      "ecmaVersion": 2020,
      "project": "tsconfig.json"
    },
    "rules": {
      "node/no-unsupported-features/es-syntax": [
        "error",
        { "ignores": ["modules"] }
      ]
    },
    "settings": {
      "node": {
        "tryExtensions": [".js", ".json", ".node", ".ts"]
      }
    }
} 